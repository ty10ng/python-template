{
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "handlers": {
      "console": {
        "enabled": true
      },
      "file": {
        "enabled": false,
        "filename": "test_project.log",
        "max_bytes": 10485760,
        "backup_count": 5
      }
    }
  },
  "api": {
    "timeout": 30,
    "max_retries": 3,
    "base_url": "https://api.example.com"
  },
  "database": {
    "pool_size": 5,
    "max_overflow": 10,
    "pool_timeout": 30
  },
  "security": {
    "mask_sensitive_data": true,
    "allowed_log_fields": ["timestamp", "level", "message", "module"]
  }
}
